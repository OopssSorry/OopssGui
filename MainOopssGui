-- Scripts:

local function VXAFAP_fake_script() -- START.Rainbower 
	local script = Instance.new('LocalScript', START)

	while wait() do
		script.Parent.BackgroundColor3 = Color3.new(1,0,0)
		for i=1,15 do
			game:GetService("RunService").RenderStepped:wait()
			script.Parent.BackgroundColor3 = Color3.new(script.Parent.BackgroundColor3.r,script.Parent.BackgroundColor3.g+(17/255),script.Parent.BackgroundColor3.b)
			wait(0.1)
		end
		wait(1)
		for i=1,15 do
			game:GetService("RunService").RenderStepped:wait()
			script.Parent.BackgroundColor3 = Color3.new(script.Parent.BackgroundColor3.r-(17/255),script.Parent.BackgroundColor3.g,script.Parent.BackgroundColor3.b)
			wait(0.1)
		end
		wait(1)
		for i=1,15 do
			game:GetService("RunService").RenderStepped:wait()
			script.Parent.BackgroundColor3 = Color3.new(script.Parent.BackgroundColor3.r,script.Parent.BackgroundColor3.g,script.Parent.BackgroundColor3.b+(17/255))
			wait(0.1)
		end
		wait(1)
		for i=1,15 do
			game:GetService("RunService").RenderStepped:wait()
			script.Parent.BackgroundColor3 = Color3.new(script.Parent.BackgroundColor3.r,script.Parent.BackgroundColor3.g-(17/255),script.Parent.BackgroundColor3.b)
			wait(0.1)
		end
		wait(1)
		for i=1,15 do
			game:GetService("RunService").RenderStepped:wait()
			script.Parent.BackgroundColor3 = Color3.new(script.Parent.BackgroundColor3.r+(17/255),script.Parent.BackgroundColor3.g,script.Parent.BackgroundColor3.b)
			wait(0.1)
		end
		wait(1)
		for i=1,15 do
			game:GetService("RunService").RenderStepped:wait()
			script.Parent.BackgroundColor3 = Color3.new(script.Parent.BackgroundColor3.r,script.Parent.BackgroundColor3.g,script.Parent.BackgroundColor3.b-(17/255))
			wait(0.1)
		end
	end
end
coroutine.wrap(VXAFAP_fake_script)()
local function OXZSC_fake_script() -- OopssGuiMain.__MAIN__ 
	local script = Instance.new('LocalScript', OopssGuiMain)

	local player = game.Players.LocalPlayer
	local code = "HYuNh2848g"
	local x = 0
	local button = script.Parent.START.Frame.TextBox
	local nick = game.Players.LocalPlayer.Name
	script.Parent.START.Frame.Invite.Image.MouseEnter:Connect(function()
		script.Parent.START.Frame.Invite.Image.UICorner.CornerRadius = UDim.new(0, 14)
	end)
	script.Parent.START.Frame.Invite.Image.MouseLeave:Connect(function()
		script.Parent.START.Frame.Invite.Image.UICorner.CornerRadius = UDim.new(1, 14)
	end)
	script.Parent.START.Frame.Invite.Copy1.MouseButton1Click:Connect(function()
		setclipboard("https://discord.gg/xGTHA6QGUk")
		script.Parent.START.Frame.Invite.Copy1.Text = "Copied!"
		wait(2)
		script.Parent.START.Frame.Invite.Copy1.Text = "Join"
	end)
	script.Parent.START.Frame.Invite.Copy2.MouseButton1Click:Connect(function()
		setclipboard("https://discord.gg/xGTHA6QGUk")
		script.Parent.START.Frame.Invite.Copy1.Text = "Copied!"
		wait(2)
		script.Parent.START.Frame.Invite.Copy1.Text = "Join"
	end)
	while true do
		if script.Parent.START.Frame.TextBox.Text == code or script.Parent.START.Frame.TextBox.Text == code.." " then
			wait(0.5)
			button.Text = "Successful"
			button.TextEditable = false
			button.TextColor3 = Color3.new(0, 0.666667, 0)
			if game.PlaceId == 537413528 then
				loadstring(game:HttpGet("https://raw.githubusercontent.com/OopssSorry/OopssGui-Script-for-Build-A-Boat/main/BuildABoatGui.lua", true))()
			elseif	game.PlaceId == 990364410 then
				loadstring(game:HttpGet("https://raw.githubusercontent.com/OopssSorry/OopssGui-Script-for-Build-A-Boat/main/SurviveTheNightGui.lua", true))()
			else
				local message = Instance.new("Hint")
				message.Text = "Game not found!!!"
				message.Parent = script.Parent
			end
			wait(0.2)
			script.Parent.START:TweenPosition(UDim2.new(0.4, -1,2, 0))
			wait(2)
			script.Parent.Parent.OopssGuiMain:Destroy()
		end
		wait(0.5)
	end
	
end
coroutine.wrap(OXZSC_fake_script)()
